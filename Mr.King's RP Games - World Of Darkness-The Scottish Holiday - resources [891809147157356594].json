    {
      "id": "892047084625215488",
      "type": "Default",
      "timestamp": "2021-09-27T13:56:38.408+00:00",
      "timestampEdited": null,
      "callEndedTimestamp": null,
      "isPinned": false,
      "content": "*Willpower*  = Resolve+Composure. 1 Willpower point per turn can be spent to add 3 dice to any pool or to add 2 to Stamina, Resolve, Composure or Defense for resistance.",
      "author": {
        "id": "294416680799567872",
        "name": "OnlyaJedi",
        "discriminator": "3499",
        "nickname": "OnlyaJedi",
        "color": "#E29E36",
        "isBot": false,
        "avatarUrl": "https://cdn.discordapp.com/avatars/294416680799567872/99ab9f059e1aee9c2f699ab371821867.png?size=512"
      },
      "attachments": [],
