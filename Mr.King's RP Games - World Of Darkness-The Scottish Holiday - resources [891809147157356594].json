    },
    {
      "id": "892047214749294602",
      "type": "Default",
      "timestamp": "2021-09-27T13:57:09.432+00:00",
      "timestampEdited": null,
      "callEndedTimestamp": null,
      "isPinned": false,
      "content": "Your character may regain one Willpower point per\nscene if her actions play out in a manner appropriate to\nher Vice. Your character may regain all spent\nWillpower if her actions play out in a manner appropriate\nto her Virtue.",
      "author": {
        "id": "294416680799567872",
        "name": "OnlyaJedi",
        "discriminator": "3499",
        "nickname": "OnlyaJedi",
        "color": "#E29E36",
        "isBot": false,
        "avatarUrl": "https://cdn.discordapp.com/avatars/294416680799567872/99ab9f059e1aee9c2f699ab371821867.png?size=512"
      },
      "attachments": [],
