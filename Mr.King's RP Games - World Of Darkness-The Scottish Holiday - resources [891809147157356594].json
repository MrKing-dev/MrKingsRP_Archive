    },
    {
      "id": "892047394756239401",
      "type": "Default",
      "timestamp": "2021-09-27T13:57:52.349+00:00",
      "timestampEdited": null,
      "callEndedTimestamp": null,
      "isPinned": false,
      "content": "Your character may regain a point of Willpower\nafter she has had a full night's rest.  If your character achieves a significant goal or performs\na particularly impressive act that affirms her sense\nof confidence, the Storyteller may choose to award a Willpower\npoint.",
      "author": {
        "id": "294416680799567872",
        "name": "OnlyaJedi",
        "discriminator": "3499",
        "nickname": "OnlyaJedi",
        "color": "#E29E36",
        "isBot": false,
